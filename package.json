{
  "name": "@primno/d365-client",
  "version": "0.2.0",
  "description": "Dynamics 365 Client for NodeJS",
  "repository": "github:primno/d365-client",
  "main": "dist/d365-client.cjs.js",
  "module": "dist/d365-client.esm.js",
  "types": "dist/d365-client.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint . --ext .ts",
    "bundle:dev": "npm run build & rollup -c",
    "bundle:prod": "npm run build & rollup -c --environment NODE_ENV:production",
    "watch": "rollup -c -w",
    "test": "nyc mocha -a -r ts-node/register test/**/*.spec.ts"
  },
  "author": "Xavier Monin",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^1.6.0",
    "@azure/msal-node-extensions": "^1.0.0-alpha.15",
    "@tediousjs/connection-string": "^0.3.0",
    "axios": "^0.25.0",
    "axios-ntlm": "^1.2.1",
    "axios-oauth-client": "^1.4.3",
    "axios-token-interceptor": "^0.2.0",
    "uri-js": "^4.4.1",
    "win-ca": "^3.4.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^16.10.3",
    "eslint": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "rollup": "^2.59.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "eslintIgnore": [
    "build",
    "dist",
    "node_modules"
  ]
}
